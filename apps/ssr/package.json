{
  "name": "ssr",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "resources": "node -r dotenv/config get-resources.js && prettier --write \"src/language-data/resources.ts\" --log-level=silent",
    "dev": "pnpm resources && next dev",
    "build": "pnpm resources && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:report": "next lint --quiet  > lint-output.txt 2>&1",
    "type-check": "tsc --noEmit",
    "type-check:report": "tsc --noEmit > type-check-output.txt 2>&1",
    "format": "prettier --write . --plugin=prettier-plugin-tailwindcss",
    "precommit": "prettier --write . && next lint --fix && tsc --noEmit ",
    "lint-staged": "npx lint-staged --config .lintstagedrc.js"
  },
  "lint-staged": {
    "*": [
      "prettier --write . --plugin=prettier-plugin-tailwindcss",
      "next lint --fix",
      "tsc --noEmit"
    ]
  },
  "dependencies": {
    "@auth/core": "^0.30.0",
    "@aws-amplify/ui-react": "^6.11.2",
    "@aws-amplify/ui-react-liveness": "^3.3.8",
    "@aws-sdk/client-rekognition": "^3.799.0",
    "@aws-sdk/client-textract": "^3.798.0",
    "@ayasofyazilim/core-saas": "latest",
    "@ayasofyazilim/saas": "0.0.165",
    "@ayasofyazilim/unirefund-saas-dev": "^0.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@repo/actions": "workspace:*",
    "@repo/saas": "workspace:*",
    "@repo/ayasofyazilim-ui": "workspace:^",
    "@repo/ui": "workspace:*",
    "@repo/utils": "workspace:*",
    "@stepperize/react": "^5.1.5",
    "@superset-ui/embedded-sdk": "^0.1.3",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-table": "8.16.0",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/leaflet": "^1.9.18",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "face-api.js": "^0.22.2",
    "geist": "1.3.0",
    "leaflet": "^1.9.4",
    "lucide-react": "0.544.0",
    "mini-svg-data-uri": "^1.4.4",
    "mrz": "^4.2.1",
    "next": "14.2.29",
    "next-auth": "5.0.0-beta.16",
    "qrcode.react": "^4.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-leaflet": "^5.0.0",
    "react-webcam": "^7.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.1.1",
    "@playwright/test": "^1.44.1",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "20.11.24",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "autoprefixer": "10.4.18",
    "lint-staged": "15.2.2",
    "postcss": "8.4.35",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  }
}
